    <connection>CAPES DS</connection>
    <sql>SELECT 
	NM_PROGRAMA_IES AS PROGRAMA
	,CD_PROGRAMA_IES
	,TIPO
	,COUNT(ID_ADD_PRODUCAO_INTELECTUAL) AS NR_ANAIS
	,AN_BASE_PRODUCAO AS AN_BASE
FROM FATO_PRODUCAO
WHERE  
	SG_ENTIDADE_ENSINO = 'UFPA'
	AND TIPO = 'TRAB EM ANAIS'
	AND AN_BASE_PRODUCAO &lt;= 2016
GROUP BY TIPO, PROGRAMA, CD_PROGRAMA_IES, AN_BASE
ORDER BY CD_PROGRAMA_IES ASC, AN_BASE ASC --MANDATORY
--ORDER BY NR_ANAIS DESC</sql>
